{
  "extends": "rackt",
  "parser": "babel-eslint",
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "rules": {
    "valid-jsdoc": 0,
    // Disable until Flow supports let and const
    "no-var": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "no-console": 0,
    "space-before-function-paren": 0,
    "quotes": 0,
    "object-curly-spacing": 0,
    "array-bracket-spacing": 0,
    "semi": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-no-undef": 2,
    "react/wrap-multilines": 2
  },
  "plugins": [
    "react"
  ]
}
